{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", ".next/**"]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "dependsOn": ["^dev", "tailwind"],
            "cache": false
        },
        "tailwind": {},
        "watch": {
            "dependsOn": ["^watch"],
            "outputs": ["dist/**"]
        }
    }
}
